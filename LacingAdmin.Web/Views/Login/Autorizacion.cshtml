
@{
    ViewBag.Title = "Autorizacion";
    ViewBag.Panel = false;
}
<head>
    <style>
        .autorizacion {
            width: 100%;
            height: 100%;
            position: fixed;
            display: block;
            margin: 0px;
            text-align: center;
            background-color: black;
        }

        .autorizacion h1 {
            font-size: 3.5rem;
        }
        .autorizacion h1, .autorizacion h3 {
            color: aliceblue;
        }
    </style>
</head>
<div class="autorizacion">
    <h1>USUARIO NO AUTORIZADO</h1>
    <h3>INICIAR SESIÓN CON OTRO USUARIO</h3>
    <button class="btn btn-warning mt-20" onclick="goToLogin()">IR AL LOGIN</button>
</div>
<script>
    function goToLogin() {
        window.location.href = "/Login/Index";
    }
</script>
